<template name="starsTemplate">
  <view class='stars-container'>
    <view class='stars'>
			<block wx:for="{{stars}}" wx:for-item="i">
				<image src="{{i?'/images/icon/star.png':'/images/icon/none-star.png'}}"></image>
			</block>
    </view>
    <text class='stars-score'>{{score}}</text>
  </view>
</template>